{
    "name": "acaronlex/laravel-calendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "acaronlex",
            "email": "acaronlex@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/support": "~7.0|~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Acaronlex\\LaravelCalendar\\": "src"
        }
    },
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/Florent-Marcel/laravel-calendar.git"
    }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Acaronlex\\LaravelCalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Acaronlex\\LaravelCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
